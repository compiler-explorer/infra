{
  "name": "compilation-lambda",
  "version": "1.0.0",
  "description": "Compiler Explorer Compilation Lambda Function",
  "main": "index.js",
  "scripts": {
    "test": "jest index.test.js",
    "test:integration": "jest integration.test.js --runInBand --forceExit",
    "test:all": "jest",
    "test:coverage": "jest --coverage"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "index.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  },
  "author": "Matt Godbolt <matt@godbolt.org>",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.0.0",
    "@aws-sdk/client-s3": "^3.0.0",
    "@aws-sdk/client-sqs": "^3.0.0",
    "@aws-sdk/client-ssm": "^3.0.0",
    "@aws-sdk/client-sts": "^3.0.0",
    "axios": "^1.6.0",
    "uuid": "^11.1.0",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "jest": "^29.7.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
